--langdef=Splus
--langmap=Splus:.s.S.R.r.q
--regex-Splus=/^[ \t]+"?([.A-Za-z][.A-Za-z0-9_]*)"?[\t]*<-[\t]*function/\1/
--regex-Splus=/^"?([.A-Za-z][.A-Za-z0-9_]*)"?[ \t]*<-/\1/

--exclude=node_modules
--exclude=gulp
--languages=-javascript
--langdef=js
--langmap=js:.js
--langmap=js:+.jsx
--regex-js=/^[ \t]*module.exports.([A-Za-z0-9_$]+)[ \s]*=[ \s]*.*$/\1/f,function/
--regex-js=/^[ \t]*exports.([A-Za-z0-9_$]+)[ \s]*=[ \s]*.*$/\1/f,function/
--regex-js=/^[ \t]*([A-Za-z0-9_$]+)[ \s]*:[ \s]*function.*$/\1/f,function/
